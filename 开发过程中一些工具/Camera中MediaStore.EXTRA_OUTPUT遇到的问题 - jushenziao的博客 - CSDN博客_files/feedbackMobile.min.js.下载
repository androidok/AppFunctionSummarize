oojs.define({name:"feedbackevents",namespace:"rs.business",deps:{},tipTime:null,fromPage:"ts",sdk:{},browsTime:null,lastTime:3,$feedbackevents:function(){var h=document.getElementById("fbIconDiv");if(!h){return false}var d=document.getElementById("fbTipDiv");var j=document.getElementById("shuntFGCon");var b=document.getElementById("shuntZD");var f=document.getElementById("shuntReturn");var e=document.getElementById("thsTMiddle");var c=document.getElementById("shuntArrow");var a=document.getElementById("container").offsetWidth;var i=document.getElementById("container").offsetHeight;var g=a/i;if(g<2){this.bind(h,"click",this.onFBDivToThsClick.proxy(this));this.bind(d,"click",this.onFBDivToThsClick.proxy(this));this.bind(e,"click",this.onTHSBacklick.proxy(this))}else{this.bind(h,"click",this.onFBDivClick.proxy(this));this.bind(d,"click",this.onFBDivClick.proxy(this));this.bind(j,"click",this.onFBFGClick.proxy(this));this.bind(b,"click",this.onFBZDClick.proxy(this));this.bind(c,"click",this.onFBReturnClick.proxy(this));this.bind(f,"click",this.onFBReturnClick.proxy(this));this.bind(e,"click",this.onTHSTMiddleClick.proxy(this))}},onFBDivToThsClick:function(){this.doShowDiv("feedbackCon");this.doHideDiv("feedbackDiv");this.doShowDiv("feedbackThs");this.doSendLogs(1,"")},onTHSBacklick:function(){this.doHideDiv("feedbackThs");this.doHideDiv("feedbackDiv");this.doHideDiv("feedbackCon");this.doSendLogs(8,"")},onFBZDClick:function(){this.doHideDiv("feedbackDiv");this.doShowDiv("feedbackThs");this.doSendLogs(3,"");this.doAdEnd()},onFBFGClick:function(){this.doHideDiv("feedbackDiv");this.doShowDiv("feedbackThs");this.doSendLogs(4,"");this.doAdEnd()},doAdEnd:function(){this.sdk=(window.baidu&&window.baidu.mobads&&window.baidu.mobads.Sdk)||(parent&&parent.baidu&&parent.baidu.mobads&&parent.baidu.mobads.Sdk);if(this.sdk&&this.sdk.AdCloseAvailable&&this.sdk.AdCloseAvailable()){this.browsTime=setInterval((function(){if(this.lastTime>0){var a=document.getElementById("thsBottom");a.inn