define("question:widget/user/nologin/nologin.js",function(o){var n=o("common:widget/js/util/tangram/tangram.js"),e=o("common:widget/js/util/log/log.js"),i=o("common:widget/js/util/event/event.js"),t=n("#nologin-reg").attr("href")+encodeURIComponent(window.location.href);n("#nologin-reg").attr("href",t),n("#nologin-login").click(function(){try{i.fire("login.log",{onLoginSuccess:function(){window.location.reload()}}),e.send({type:2014,page:"question",pos:"nologin",action:"click"})}catch(o){"undefined"!=typeof alog&&alog("exception.fire","catch",{msg:o.message,path:"question:widget/user/nologin/nologin.js",method:"",ln:21})}}),n("#nologin-reg").click(function(){try{e.send({type:2014,page:"question",pos:"nologin-reg",action:"click"})}catch(o){"undefined"!=typeof alog&&alog("exception.fire","catch",{msg:o.message,path:"question:widget/user/nologin/nologin.js",method:"",ln:30})}})});