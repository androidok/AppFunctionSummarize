define("question:widget/ask/pc-exp/ask-img.js",function(e){var i=e("common:widget/lib/jquery/jquery.js"),t=(e("common:widget/js/ui/dialog/dialog.js"),e("common:widget/js/logic/category/category.js"),e("common:widget/js/util/log/log.js"),e("question:widget/read-opt/img-preview/preview.js")),o=i(".q-img-ul"),g=i(".q-img-con"),n=o.width(),a=g.width();a>=n&&(i(".q-img-prev-wp").remove(),i(".q-img-next-wp").remove()),i(".q-img-next").on("click",function(e){try{var i=o.position().left;o.animate(n+i>=2*a?{left:"-"+(a-i)+"px"}:{left:"-"+(n-a)+"px"})}catch(e){"undefined"!=typeof alog&&alog("exception.fire","catch",{msg:e.message,path:"question:widget/ask/pc-exp/ask-img.js",method:"",ln:41})}}),i(".q-img-prev").on("click",function(){try{var e=o.position().left;o.animate(-a>=e?{left:e+a+"px"}:{left:"0px"})}catch(i){"undefined"!=typeof alog&&alog("exception.fire","catch",{msg:i.message,path:"question:widget/ask/pc-exp/ask-img.js",method:"",ln:57})}}),new t({$el:".q-img-li",imgAttr:"data-src"}),i(".replyask-box .ikqb_img_alink").click=null,new t({$el:".replyask-box .ikqb_img_alink",imgAttr:"href"})});