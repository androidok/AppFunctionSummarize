$(document).ready(function(){var t="/"+gon.user_project,r=gon.ref.split("/");for(x in r)r[x]=encodeURIComponent(r[x]);var e=/^(https?:\/\/|\/\/)/i,i=/(\.(png|jpg|jpeg|gif|svg))$/i;$("a.md_relative_url").each(function(){var i=$(this).attr("href");e.test(i)||$(this).attr("href",t+"/blob/"+r.join("/")+"/"+i)}),$("img.md_relative_url").each(function(){var n=$(this).attr("src");e.test(n)||i.test(n)&&$(this).attr("src",t+"/raw/"+r.join("/")+"/"+n)})});